{
  "configurations": {
    "Vim - run a test": {
      "adapter": "CodeLLDB",
      "configuration": {
        "type":    "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/../xbuild/Debug/console/console",
        "cwd": "${workspaceRoot}/../xbuild/Debug/console",
        "externalConsole": true,
        "stopAtEntry": true,
        "MIMode": "lldb",
        "logging": {
          "engineLogging": false
        }
      }
    }
  }
}
